    "DefaultConnection": "Server=localhost;Port=3306;Database=isbasvurudb;Uid=db_user;Pwd="
  },
  "EmailSettings": {
    "SmtpServer": "smtp.gmail.com",
    "Port": 587,
    "SenderEmail": "ornek@email.com",
